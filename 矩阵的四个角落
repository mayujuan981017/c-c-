//表示矩阵的最左上角（可参照牛客网wyh矩阵）
int k=n/2+1;//(矩阵是按照从1-n*n排列的)
for(int i=1;i<k;i++)
{
    for(int j=1;j<=k-i;j++)
}
//最右上角
for(int i=1;i<k;i++)
{
    for(int j=k+i;j<=n;j++)
}
//最左上角
for(int i=k+1;i<=n;i++)
{
    for(int j=1;j<=i-k;j++)
}
//Z最左下角
for(int i=k+1;i<=n;i++)
{
    for(int j=n-(i-k)+1;j<=n;j++)
}
//最右下角
