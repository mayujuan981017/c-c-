#include <cstdio>
#include <iostream>
using namespace std;
int main()
{
    int n,a[100005];
    while(cin>>n)
    {
            int pos=1,cnt=0;//1刚开始的位置
            int t=2*n+1;
            while(1)
            {
                pos=pos*2%t;
                cnt++;
                if(pos==1)
                {
                    break;
                }
            }
            cout<<cnt<<endl;
    }
}
